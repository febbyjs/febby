{
  "name": "febby",
  "version": "0.2.14",
  "description": "microservice framework for building application services",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --updateSnapshot",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "docs": "./node_modules/.bin/typedoc --out docs --mode modules ./index.ts",
    "build": "./node_modules/.bin/tsc",
    "build:local": "npm test && npm start && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/febbyjs/febby.git"
  },
  "keywords": [
    "rest",
    "configuration",
    "minimalistic",
    "microservices",
    "node framework"
  ],
  "author": "Vasu Vanka <vanka.vasu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/febbyjs/febby/issues"
  },
  "homepage": "https://github.com/febbyjs/febby#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonschema": "^1.4.0",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "redis": "^3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "^5.5.35",
    "@types/morgan": "^1.7.37",
    "@types/node": "^14.14.37",
    "jest": "^26.4.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  }
}
