{
  "name": "febby",
  "version": "0.3.4",
  "description": "microservice framework for building restful services",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --updateSnapshot",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "docs": "./node_modules/.bin/typedoc",
    "build": "./node_modules/.bin/tsc",
    "build:local": "npm test && npm start && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/febbyjs/febby.git"
  },
  "keywords": [
    "rest",
    "restful",
    "configuration",
    "minimalistic",
    "microservices",
    "node framework"
  ],
  "author": "Vasu Vanka <vanka.vasu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/febbyjs/febby/issues"
  },
  "homepage": "https://github.com/febbyjs/febby#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.9",
    "mongoose": "^6.0.7",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/helmet": "0.0.48",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^27.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.1",
    "jest": "^27.2.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  }
}
