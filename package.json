{
  "name": "febby",
  "version": "0.5.0",
  "description": "microservice framework for building restful services",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "./node_modules/.bin/tsc",
    "test": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "docs": "./node_modules/.bin/typedoc",
    "build": "./node_modules/.bin/tsc",
    "build:local": "npm test && npm start && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/febbyjs/febby.git"
  },
  "keywords": [
    "rest",
    "restful",
    "open-api",
    "auto generate",
    "configuration",
    "minimalist",
    "microservices",
    "node.js framework"
  ],
  "author": "Vasu Vanka <vanka.vasu@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/febbyjs/febby/issues"
  },
  "homepage": "https://github.com/febbyjs/febby#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "express-openapi-validator": "^5.0.6",
    "helmet": "^5.1.1",
    "ioredis": "^5.2.2",
    "js-yaml": "^4.1.0",
    "mongoose": "^6.0.7",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.3",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.2.1",
    "typedoc": "^0.23.10",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
